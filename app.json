{
  "expo": {
    "name": "ProjetDriver",
    "slug": "ProjetDriver",
    "version": "1.0.0",
    
    "notification": {
      "icon": "./assets/Logo.png",
      "color": "#ffffff"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/Logo.png",
          "color": "#ffffff"
         
        }
      ]
    ],
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ProjetDriver",
      "config": {
        "googleMapsApiKey": "AIzaSyBQivdVNxU7quHhW_ARw2VuXKmHVwXhNMk"
      }
    },
    "android": {
    
      "googleServicesFile": "./google-services.json",
      "package": "com.ProjetDriver",
      "jsEngine": "hermes",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.NOTIFICATIONS",
        "android.permission.VIBRATE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBQivdVNxU7quHhW_ARw2VuXKmHVwXhNMk"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "57e70b0c-f485-44cc-bfb9-b6868dcbde3f"
      }
    }
  }
}
